}

model Match {
  id                                                  String          @id(map: "matchs_pkey") @db.Uuid
  user_created                                        String?         @db.Uuid
  date_created                                        DateTime?       @db.Timestamptz(6)
  user_updated                                        String?         @db.Uuid
  date_updated                                        DateTime?       @db.Timestamptz(6)
  player1_id                                          String          @db.Uuid
  player2_id                                          String          @db.Uuid
  winner_id                                           String?         @db.Uuid
  status                                              String          @default("PENDING") @db.VarChar(255)
  created_at                                          DateTime        @default(now()) @db.Timestamptz(6)
  ended_at                                            DateTime?       @db.Timestamptz(6)
  rng_seed                                            BigInt
  users_matches_player1_idTousers                     User            @relation("matches_player1_idTousers", fields: [player1_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "matchs_player1_id_foreign")
  users_matches_player2_idTousers                     User            @relation("matches_player2_idTousers", fields: [player2_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "matchs_player2_id_foreign")
  directus_users_matches_user_createdTodirectus_users directus_users? @relation("matches_user_createdTodirectus_users", fields: [user_created], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "matchs_user_created_foreign")
  directus_users_matches_user_updatedTodirectus_users directus_users? @relation("matches_user_updatedTodirectus_users", fields: [user_updated], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "matchs_user_updated_foreign")
  users_matches_winner_idTousers                      User?           @relation("matches_winner_idTousers", fields: [winner_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "matchs_winner_id_foreign")

  @@map("matches")
}

model SoloGame {
  id                                                     String          @id @db.Uuid
  user_created                                           String?         @db.Uuid
  date_created                                           DateTime?       @db.Timestamptz(6)
  user_updated                                           String?         @db.Uuid
  date_updated                                           DateTime?       @db.Timestamptz(6)
  created_at                                             DateTime        @default(now()) @db.Timestamptz(6)
  difficulty                                             String?         @db.VarChar(50)
  duration_ms                                            BigInt?
  ended_at                                               DateTime?       @db.Timestamptz(6)
  level                                                  Int             @default(1)
  lines_cleared                                          Int             @default(0)
  player_id                                              String          @db.Uuid
  rng_seed                                               BigInt?
  score                                                  Int             @default(0)
  status                                                 String          @default("PENDING") @db.VarChar(20)
  users                                                  User            @relation(fields: [player_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  directus_users_solo_games_user_createdTodirectus_users directus_users? @relation("solo_games_user_createdTodirectus_users", fields: [user_created], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "solo_games_user_created_foreign")
  directus_users_solo_games_user_updatedTodirectus_users directus_users? @relation("solo_games_user_updatedTodirectus_users", fields: [user_updated], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "solo_games_user_updated_foreign")

  @@index([player_id], map: "idx_solo_games_player")
  @@map("solo_games")
}
