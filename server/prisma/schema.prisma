model Room {
  id                                                String          @id @db.Uuid
  user_created                                      String?         @db.Uuid
  date_created                                      DateTime?       @db.Timestamptz(6)
  user_updated                                      String?         @db.Uuid
  date_updated                                      DateTime?       @db.Timestamptz(6)
  leader_id                                         String          @db.Uuid
  opponent_id                                       String?         @db.Uuid
  created_at                                        DateTime        @default(now()) @db.Timestamptz(6)
  name                                              String?         @db.VarChar(255)
  users_rooms_leader_idTousers                      User            @relation("rooms_leader_idTousers", fields: [leader_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "rooms_leader_id_foreign")
  users_rooms_opponent_idTousers                    User?           @relation("rooms_opponent_idTousers", fields: [opponent_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "rooms_opponent_id_foreign")
  directus_users_rooms_user_createdTodirectus_users directus_users? @relation("rooms_user_createdTodirectus_users", fields: [user_created], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "rooms_user_created_foreign")
  directus_users_rooms_user_updatedTodirectus_users directus_users? @relation("rooms_user_updatedTodirectus_users", fields: [user_updated], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "rooms_user_updated_foreign")

  @@map("rooms")
}

model User {
  id                                                String          @id @db.Uuid
  user_created                                      String?         @db.Uuid
  date_created                                      DateTime?       @db.Timestamptz(6)
  user_updated                                      String?         @db.Uuid
  date_updated                                      DateTime?       @db.Timestamptz(6)
