{
  "name": "backend",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "dev": "./scripts/dev.sh",
    "test": "node --test",
    "coverage": "c8 --check-coverage --statements 70 --functions 70 --lines 70 --branches 50 node --test",
    "prisma:generate": "prisma generate",
    "prisma:db-push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "format": "prettier --write \"**/*.{js,json,css,md}\"",
    "lint": "eslint ."
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "express": "~4.16.1",
    "express-jwt": "^8.5.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "nodemon": "^3.1.9",
    "pg-promise": "^11.10.2",
    "pino": "^10.1.0",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "c8": "^10.1.2",
    "prisma": "^6.1.0"
  },
  "c8": {
    "all": true,
    "include": [
      "src/dao/**/*.js",
      "src/services/userService.js",
      "src/services/matchHistoryService.js"
    ],
    "exclude": [
      "test/**"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
